{
  "$schema": "https://schemas.wp.org/trunk/wp-env.json",
  "env": {
    "tests": {
      "core": null,
      "phpVersion": "7.4",
      "config": {
        "WP_DEBUG_DISPLAY": false,
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": true,
        "SCRIPT_DEBUG": true
      },
      "mappings": {
        "wp-content/debug.log": "./debug.log"
      },
      "plugins": [
        ".",
        "../premium-plugins/js_composer"
      ],
      "themes": [
        "../premium-themes/flatsome",
        "../premium-themes/Divi"
      ]
    }
  }
}