{
	"parser": "@typescript-eslint/parser",
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"plugins": [
		"react",
		"react-hooks",
		"@typescript-eslint"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:@wordpress/eslint-plugin/recommended"
	],
	"ignorePatterns": [
		"*.js"
	],
	"rules": {
		"@typescript-eslint/no-namespace": "off"
	},
	"globals": {
		"JSX": true,
		"NodeJS": true
	}
}
