{
  "compilerOptions": {
    "outDir": "./build/",
    "target": "ES2020",
    "jsx": "react-jsx",
    "module": "ESNext",
    "noImplicitAny": false,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "moduleResolution": "node"
  },
  "include": [
    "./src/**/*"
  ]
}